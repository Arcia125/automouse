{
  "name": "automouse",
  "version": "1.10.2",
  "description": "Mouse CLI",
  "engines": {
    "node": ">=10"
  },
  "main": "index.js",
  "bin": {
    "automouse": "bin/automouse"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write **/*.js",
    "prepare-commit": "lint-staged",
    "test": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arcia125/automouse.git"
  },
  "keywords": [
    "mouse",
    "automation",
    "cursor",
    "CLI"
  ],
  "preferGlobal": true,
  "author": "Kevin Hallett",
  "directories": {
    "bin": "bin",
    "lib": "src"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arcia125/automouse/issues"
  },
  "homepage": "https://github.com/Arcia125/automouse#readme",
  "dependencies": {
    "minimist": "^1.2.0",
    "robotjs": "^0.5.1"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare-commit"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint"
    ],
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
